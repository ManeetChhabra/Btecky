#include <bits/stdc++.h>

using namespace std;



class Solution{
public:
	// Function to check if the
	// Pythagorean triplet exists or not
	bool checkTriplet(int arr[], int n) {
	    // code here
	    sort(arr,arr+n);
        for(int i = 0;i<n;i++){
            arr[i]= arr[i]*arr[i];
        }
        for(int i = 0; i<n; i++){
            for(int j = 1;j<n;j++){
                for(int y = 2;y<n;y++){
                    if(arr[i]+arr[j]== arr[y]){
                        return 1;
                    }
                }
            }
        }
        return 0;
    
	       
    
	}
};



int main() {
    int t;
    cin >> t;
    while (t--) {
        int n, i;
        cin >> n;
        int arr[n];
        for (i = 0; i < n; i++) {
            cin >> arr[i];
        }
        Solution ob;
        auto ans = ob.checkTriplet(arr, n);
        if (ans) {
            cout << "Yes\n";
        } else {
            cout << "No\n";
        }
    }
    return 0;
}
